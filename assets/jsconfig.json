{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"*": ["*"]
		},
		"target": "es2024",
		"lib": ["dom", "es2024"],
		"moduleResolution": "node",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",
	}
}
